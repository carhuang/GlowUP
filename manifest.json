{
    "name": "Blank new tab page",
    "description": "Override the new tab page with a blank one",
    "version": "0.2",
    "incognito": "split",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },

    "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["content.js"]
        }
      ],

    "manifest_version": 2,
    "page_action": {
        "default_icon": "img.png",
        "default_popup": "popup.html",
        "default_title": "First Chrome Extension"
    }
}